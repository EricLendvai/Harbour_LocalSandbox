    "version": "0.2.0",
    "configurations": [
        {
            "type": "harbour-dbg",
            "request": "attach",
            "name": "Attach Program Harbour_LocalSandbox.exe",
            // "preLaunchTask": "CompileDebug",
            // "postDebugTask": "",
            "windows":{
                "program": "R:\\Harbour_websites\\fcgi_Harbour_LocalSandbox\\backend\\FCGIHarbour_LocalSandbox.exe",
                "sourcePaths": [
                    "${workspaceFolder}",
                    "R:\\Harbour_ORM\\",
                    "R:\\Harbour_EL\\"
                ]
            },
            "linux":{
                "program": "/var/www/Harbour_websites/fcgi_Harbour_LocalSandbox/backend/FCGIHarbour_LocalSandbox.exe",
                "sourcePaths": [
                    "${workspaceFolder}",
                    "/src/Harbour_ORM/",
                    "/src/Harbour_EL/"
                ]
            }
        }
    ]
}
